{
  "name": "volun-link-fullstack",
  "version": "1.0.0",
  "description": "VolunLink - Full Stack Community Service Management System",
  "scripts": {
    "dev": "concurrently -n \"BACKEND,FRONTEND\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd volun-link-main && npm run dev",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd volun-link-main && npm run dev",
    "install:all": "npm install && cd backend && npm install && cd ../volun-link-main && npm install",
    "build": "cd volun-link-main && npm run build"
  },
  "keywords": ["volunteer", "community", "service", "management", "fullstack"],
  "author": "Mayur",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
